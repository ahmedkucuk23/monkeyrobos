<div id="<%= dom_id contact %>">
  <p>
    <strong>Name:</strong>
    <%= contact.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.email %>
  </p>

  <p>
    <strong>Project name:</strong>
    <%= contact.project_name %>
  </p>

  <p>
    <strong>Project supply:</strong>
    <%= contact.project_supply %>
  </p>

  <p>
    <strong>Project price:</strong>
    <%= contact.project_price %>
  </p>

  <p>
    <strong>Info:</strong>
    <%= contact.info %>
  </p>

</div>
